<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ContextMenu?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.demo.controllers.ChatWindow">
   <children>
      <TreeView fx:id="commentWindow" layoutX="14.0" layoutY="14.0" onMouseClicked="#loadComment" prefHeight="200.0" prefWidth="571.0">
         <contextMenu>
            <ContextMenu fx:id="commentContextMenu">
            </ContextMenu>
         </contextMenu>
      </TreeView>
      <TextArea fx:id="commentBody" layoutX="15.0" layoutY="243.0" prefHeight="118.0" prefWidth="571.0" />
      <Button fx:id="addCommentButton" layoutX="281.0" layoutY="367.0" mnemonicParsing="false" onAction="#insertComment" text="Add" />
      <Text layoutX="15.0" layoutY="239.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Message Field" />
   </children>
</AnchorPane>
